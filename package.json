{
  "name": "react-material-electron",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "description": "a small app build on react, material-ui and electron",
  "author": {
    "name": "liudonghua",
    "email": "liudonghua123@gmail.com",
    "url": "http://liudonghua.com"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "electron-default-menu": "^1.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^7.0.0",
    "material-design-icons": "^3.0.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-app-rewired": "^1.6.2",
    "react-diff-viewer": "^1.0.4",
    "react-dom": "^16.6.0",
    "react-list-drag-and-drop": "^0.8.1",
    "react-monaco-editor": "^0.22.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "segment": "^0.1.3",
    "typeface-roboto": "0.0.54",
    "update-electron-app": "^1.3.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "cross-env NODE_ENV=production CI=false react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "electron": "electron .",
    "electron-start": "node public/electron-wait-react",
    "react-start": "react-app-rewired start",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && cross-env NODE_ENV=production CI=false electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --fix src/pages/Home.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.29",
    "@electron-forge/maker-deb": "^6.0.0-beta.29",
    "@electron-forge/maker-flatpak": "^6.0.0-beta.29",
    "@electron-forge/maker-rpm": "^6.0.0-beta.29",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.29",
    "@electron-forge/maker-zip": "^6.0.0-beta.29",
    "cross-env": "^5.2.0",
    "electron": "3.0.10",
    "electron-builder": "^20.29.0",
    "electron-builder-squirrel-windows": "^20.30.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "foreman": "^3.0.1"
  },
  "build": {
    "appId": "com.liudonghua.react-material-electron",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "artifactName": "${name}-${version}-${platform}-${arch}.${ext}",
      "target": [
        {
          "target": "zip",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "artifactName": "${name}-${version}-${platform}-${arch}.${ext}",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "artifactName": "${name}-${version}-${platform}-${arch}.${ext}",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
